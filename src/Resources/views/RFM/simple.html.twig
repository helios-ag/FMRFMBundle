{% extends '@FMRFM/RFM/base.html.twig' %}
{% block script %}
    <script type="application/javascript">
        $(function() {
            $('.fm-container').richFilemanager({
                baseUrl: '{{ app.request.getSchemeAndHttpHost() ~ '/' ~ prefix }}',
                configUrl: '{{ url('rfm_config', {'instance': instance}) }}'
            });
        });
    </script>
{% endblock %}
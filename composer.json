{
    "name": "helios-ag/fm-rfm-bundle",
    "description": "FMRichfilemanager bundle, adds Richfilemanager file manager to your Symfony project",
    "keywords": ["Richfilemanager","file manager","wysiwyg"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/helios-ag/FMRFMBundle",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Al Ganiev",
            "email": "helios.ag@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/helios-ag/FMRFMBundle/contributors"
        }
    ],
    "archive": {
      "exclude": ["/Tests"]
    },
    "require": {
        "php":                                 "^7.1",
        "symfony/framework-bundle":            "^3.4|^4.0",
        "symfony/twig-bundle":                 "^3.4|^4.0",
        "symfony/form":                        "^3.4|^4.0",
        "symfony/asset":                       "^3.4|^4.0",
        "servocoder/richfilemanager":          "^2.7"

    },
    "require-dev" : {
        "symfony/phpunit-bridge": "^4.0",
        "matthiasnoback/symfony-config-test":  "^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^2.0",
        "symfony/finder": "^3.4|^4.0",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "suggest": {
        "helios-ag/fm-tinymce-bundle": "TinyMCE Bundle",
        "helios-ag/fm-summernote-bundle": "Summernote Bundle",
        "friendsofsymfony/ckeditor-bundle": "CKEditor Bundle"
    },
    "autoload": {
        "psr-4": { "FM\\RFMBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "FM\\RFMBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
